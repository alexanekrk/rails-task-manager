<h1>Edit task</h1>

<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.submit %>
<% end %>

<br>
<br>

<input type="checkbox" name="complete">
    <label for="complete"> Complete </label>

<br>
<br>

<%= link_to "Back to tasks", tasks_path %>
